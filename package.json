{
  "name": "blockchain",
  "version": "1.0.0",
  "description": "an example of blockchain implementation with node.js",
  "main": "index.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "run": "nodemon --exec npm run _run",
    "_run": "npm run build && npm run start",
    "build": "tsc -p tsconfig.json",
    "start": "node ./index.js",
    "clean": "node ./clean.js"
  },
  "keywords": [
    "blockchain"
  ],
  "author": "mdepasquale21",
  "license": "ISC",
  "dependencies": {
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "del": "^6.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "nodemonConfig": {
    "ignore": [],
    "ext": "ts"
  }
}
